{
  "name": "axios-plugin",
  "private": true,
  "type": "module",
  "preinstall": "npx only-allow pnpm",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.2",
    "@types/jest": "^29.5.1",
    "axios": "^1.7.7",
    "axios-mock-adapter": "^1.21.4",
    "babel-jest": "^29.5.0",
    "husky": "^9.1.6",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.10",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "ts-morph": "^24.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "test": "jest --bail",
    "prepare": "husky install",
    "build": "node scripts/build.js",
    "gen-doc": "node scripts/generateApiDoc.js"
  },
  "husky": {
    "hooks": {
      "lint-staged": "npx lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.test.ts": ["pnpm run test"]
